
<% if Array.isArray(@list) && @list.length > 0: %>
    
    <div class="sidebar-section">

        <h3 class="h1">Further Reading</h3>

        <ul>
            <% for link in @list: %>
                <li><a href="<%= link.url %>"><%= link.label %></a></li>
            <% end %>
        </ul>

    </div>

<% end %>
